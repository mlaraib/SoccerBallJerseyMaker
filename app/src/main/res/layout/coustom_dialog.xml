<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent">


    <ImageView android:id="@+id/facebook"
               android:layout_width="50dp"
               android:layout_height="50dp"
               android:layout_marginLeft="30dp"
               android:layout_marginTop="30dp"
               android:layout_marginBottom="30dp"
               android:src="@drawable/facebooicon" />

    <ImageView android:id="@+id/twitter"
               android:layout_width="50dp"
               android:layout_height="50dp"
               android:layout_marginTop="30dp"
               android:layout_marginRight="30dp"
               android:layout_marginBottom="30dp"
               android:src="@drawable/twittericon"
               android:layout_centerHorizontal="true" />

    <ImageView android:id="@+id/whatsapp"
               android:layout_width="50dp"
               android:layout_height="50dp"
               android:layout_marginTop="30dp"
               android:layout_marginRight="30dp"
               android:layout_marginBottom="30dp"
               android:src="@drawable/whatsapp"
               android:layout_alignParentRight="true" />

    <ImageView android:id="@+id/viber"
               android:layout_width="50dp"
               android:layout_height="50dp"
               android:layout_marginLeft="30dp"
               android:layout_marginTop="10dp"
               android:layout_marginBottom="30dp"
               android:src="@drawable/viber"
               android:layout_below="@+id/facebook" />

    <ImageView android:id="@+id/instagram"
               android:layout_width="50dp"
               android:layout_height="50dp"
               android:layout_marginTop="10dp"
               android:layout_marginRight="30dp"
               android:layout_marginBottom="30dp"
               android:src="@drawable/instagram"
               android:layout_below="@+id/twitter"
               android:layout_centerHorizontal="true" />

    <ImageView android:id="@+id/gmail"
               android:layout_width="50dp"
               android:layout_height="50dp"
               android:layout_marginTop="10dp"
               android:layout_marginRight="30dp"
               android:layout_marginBottom="30dp"
               android:src="@drawable/gmail"
               android:layout_below="@+id/whatsapp"
               android:layout_alignParentRight="true" />

    <Button android:textColor="#000"
            android:id="@+id/dialogButtonCancle"
            android:background="#fff"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginRight="5dp"
            android:text=" Cancel "
            android:layout_below="@+id/viber" />
    

</RelativeLayout>
